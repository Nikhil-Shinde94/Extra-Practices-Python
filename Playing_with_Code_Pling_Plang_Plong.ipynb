      "source": [
        "def covert(num):\n",
        "\n",
        "  if num % 3 == 0:\n",
        "    return \"Pling\"\n",
        "  if num % 5 == 0:\n",
        "    return \"Plang\"\n",
        "  if num % 7 == 0:\n",
        "    return \"Plong\"\n",
        "  else:\n",
        "    return str(num)\n",
        "num = int(input(\"Enter Number:\"))\n",
        "covert(num)"
